{
  "tasks": {
    "start": "deno -A --env main.ts",
    "fmt": "deno fmt --unstable-sql",
    "check": "deno check --unstable-raw-imports",
    "test": "deno test -A --unstable-raw-imports",
    "precommit": {
      "description": "Formats and checks code",
      "dependencies": ["fmt", "test"]
    }
  },
  "imports": {
    "@libsql/client": "npm:@libsql/client@^0.15.15",
    "@std/assert": "jsr:@std/assert@^1.0.16",
    "@tensorflow/tfjs": "npm:@tensorflow/tfjs@^4.22.0",
    "@tensorflow-models/universal-sentence-encoder": "npm:@tensorflow-models/universal-sentence-encoder@^1.3.3",
    "@tensorflow/tfjs-backend-cpu": "npm:@tensorflow/tfjs-backend-cpu@^4.22.0"
  }
}
